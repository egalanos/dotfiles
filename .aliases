# .bashrc

alias l.='ls -d .* --color=auto'
alias ll='ls -l --color=auto'
alias ls='ls --color=auto'
alias la='ls -Al --color=auto'

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# pnpm
alias pn='pnpm'
alias pd='pnpm dev'
alias pb='pnpm build'
alias pt='pnpm test'
alias pi='pnpm install'
alias pa='pnpm add'
alias pad='pnpm add -D'

# git
alias g='git'
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gcm='git commit -m'
alias gp='git push'
